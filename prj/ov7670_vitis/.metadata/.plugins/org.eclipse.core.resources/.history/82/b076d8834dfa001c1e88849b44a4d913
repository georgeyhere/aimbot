#ifndef I2C_MASTER_H
#define I2C_MASTER_H

#include <stdio.h>
#include "xil_types.h"
#include "xil_assert.h"
#include "xstatus.h"

/************************** Constant Definitions ****************************/

/**************************** Type Definitions ******************************/

/**
 * Statistics for the I2c Driver
 */
typedef struct {
	u32 BytesTransmitted;
	u32 BytesReceived;
	u32 ReceiveOverrunErrors;
} I2C_Stats;



#endif
